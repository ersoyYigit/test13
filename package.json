    "@remix-run/css-bundle": "^2.6.0",
    "@remix-run/express": "^2.6.0",
    "@remix-run/node": "^2.6.0",
    "@remix-run/react": "^2.6.0",
    "@tabler/icons-react": "^2.47.0",
    "axios": "^1.6.7",
    "clsx": "^2.1.0",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.10",
    "express": "^4.18.2",
    "i18next": "^23.10.0",
    "i18next-browser-languagedetector": "^7.2.0",
